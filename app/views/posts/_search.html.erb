<%= search_form_for search, url: new_post_path do |f| %>
  <%= f.label :name, "ユーザー名" %>
  <%= f.search_field :last_name_or_first_name_or_last_name_kana_or_first_name_kana_cont, autofocus: true, placeholder: "検索キーワード", class:"form-control" %>
  <%= f.submit "検索", class:"btn btn-block btn-primary" %>
<% end %>